    "eslint": "^5.16.0",
    "mocha": "^6.1.4",
    "mocha-context": "^0.1.5",
    "rollup": "^1.14.1",
    "tempdir-yaml": "^0.2.1"
  },
  "peerDependencies": {
    "rollup": ">=0.61"
  },
  "repository": "eight04/rollup-plugin-inline-js",
